@prefix : <http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies> .

<http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies> rdf:type owl:Ontology ;
                                                                       owl:imports <http://www.w3.org/2006/time#2016> ;
                                                                       rdfs:comment "An ontology to describe meteo" .

#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#cdt:ucum
:cdt:ucum rdf:type rdfs:Datatype .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#cdt:ucumunit
:cdt:ucumunit rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourDirectionVent
:aPourDirectionVent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCaracteristics ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :MesureMeteo ;
                    rdfs:range :WindDirectionValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourLieuDit
:aPourLieuDit rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourPluviometrie
:aPourPluviometrie rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :aPourPrécipitation ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:range :RainValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourPrécipitation
:aPourPrécipitation rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCaracteristics ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:range :SnowLevelValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourStationMétéo
:aPourStationMétéo rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :MesureMeteo ;
                   rdfs:range :StationMeteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourUnité
:aPourUnité rdf:type owl:ObjectProperty ;
            owl:inverseOf :mesureLaGrandeurPhysique ;
            rdfs:domain :MesurePhysique ;
            rdfs:range :Unit .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourUnitéHumidité
:aPourUnitéHumidité rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :aPourUnité .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourUnitéPression
:aPourUnitéPression rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :aPourUnité .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourUnitéTempérature
:aPourUnitéTempérature rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :aPourUnité .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourUnitéVitesse
:aPourUnitéVitesse rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :aPourUnité .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourVitesseVent
:aPourVitesseVent rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCaracteristics ;
                  rdfs:domain :MesureVitesse ;
                  rdfs:range :WindSpeedValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#altitude
:altitude rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :PhenomeneMeteoExtreme ;
          rdfs:range :Précipitations .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#correspondAuLieu
:correspondAuLieu rdf:type owl:ObjectProperty ;
                  rdfs:domain :MesureMeteo ;
                  rdfs:range :Lieu .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#estDansLaCommune
:estDansLaCommune rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :estDansLeLieu ;
                  rdfs:domain :Lieu ;
                  rdfs:range :Commune .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#estDansLaRegion
:estDansLaRegion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :estDansLeLieu ;
                 rdfs:domain :Lieu ;
                 rdfs:range :Region .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#estDansLeDepartement
:estDansLeDepartement rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :estDansLeLieu ;
                      rdfs:domain :Lieu ;
                      rdfs:range :Departement .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#estDansLeLieu
:estDansLeLieu rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#hasCaracteristics
:hasCaracteristics rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#hasSnowLevel
:hasSnowLevel rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :aPourPrécipitation .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#hasSunshine
:hasSunshine rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasCaracteristics ;
             rdf:type owl:FunctionalProperty ;
             rdfs:range :SunShineValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#hasTemperature
:hasTemperature rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasCaracteristics ;
                rdf:type owl:FunctionalProperty ;
                rdfs:range :TemperatureValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#hasVisibility
:hasVisibility rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasCaracteristics .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#latitude
:latitude rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Commune ,
                      :Lieu-Dit ,
                      :StationMeteo ;
          rdfs:range :DegréAngulaire .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#longitude
:longitude rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Commune ,
                       :Lieu-Dit ,
                       :StationMeteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#mesureLaGrandeurPhysique
:mesureLaGrandeurPhysique rdf:type owl:ObjectProperty ;
                          rdfs:domain :Unit ;
                          rdfs:range :MesurePhysique .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#temperature
:temperature rdf:type owl:ObjectProperty ;
             rdfs:domain :MesureMeteo ;
             rdfs:range :MesureTemperature .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:FunctionalProperty ;
                      rdfs:range :VisibilityValuePartition .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourDate
:aPourDate rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :MesureMeteo ,
                       :PrevisionMeteo ;
           rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourDateCreation
:aPourDateCreation rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :aPourDate ;
                   rdfs:domain :MesureMeteo ,
                               :PrevisionMeteo ;
                   rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourDatePrevisionnelle
:aPourDatePrevisionnelle rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :aPourDate ;
                         rdfs:domain :PrevisionMeteo ;
                         rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#aPourValeur
:aPourValeur rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:double .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#commentaire
:commentaire rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeur
:valeur rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeurAngle
:valeurAngle rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeurHumidité
:valeurHumidité rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeurPression
:valeurPression rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeurTempérature
:valeurTempérature rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#valeurVitesse
:valeurVitesse rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Anticyclone
:Anticyclone rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :aPourVitesseVent ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:unionOf ( :HighWindSpeed
                                                                     :VeryHighWindSpeed
                                                                   )
                                                     ]
                                 ] ;
             rdfs:subClassOf :PhenomeneMeteoExtreme .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Blizzard
:Blizzard rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :aPourVitesseVent ;
                                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                                           owl:unionOf ( :HighWindSpeed
                                                                                         :VeryHighWindSpeed
                                                                                       )
                                                                         ]
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasSnowLevel ;
                                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                                           owl:unionOf ( :HighSnowLevel
                                                                                         :VeryHighSnowLevel
                                                                                       )
                                                                         ]
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasVisibility ;
                                                       owl:allValuesFrom :VeryLowVisibility
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :PhenomeneMeteoExtreme .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Canicule
:Canicule rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :aPourPluviometrie ;
                                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                                           owl:unionOf ( :LowRain
                                                                                         :VeryLowRain
                                                                                       )
                                                                         ]
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasTemperature ;
                                                       owl:allValuesFrom :VeryHighTemperature
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :PhenomeneMeteoExtreme .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#CloudSunny
:CloudSunny rdf:type owl:Class ;
            rdfs:subClassOf :SunShineValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Clowdy
:Clowdy rdf:type owl:Class ;
        rdfs:subClassOf :SunShineValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Commune
:Commune rdf:type owl:Class ;
         rdfs:subClassOf :Lieu ;
         owl:disjointWith :Departement ,
                          :Lieu-Dit ,
                          :Region .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Cyclone
:Cyclone rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:unionOf ( :Ouragan
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :aPourVitesseVent ;
                                               owl:allValuesFrom :VeryHighWindSpeed
                                             ]
                                           )
                             ] ;
         rdfs:subClassOf :PhenomeneMeteoExtreme .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#DegréAngulaire
:DegréAngulaire rdf:type owl:Class ;
                rdfs:subClassOf :Unit ;
                rdfs:comment "ATTENTION : On sait pas si on fait une classe ou autre chose pour représenter un degré" .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#DegréCelcius
:DegréCelcius rdf:type owl:Class ;
              rdfs:subClassOf :Unit .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Departement
:Departement rdf:type owl:Class ;
             rdfs:subClassOf :Lieu ;
             owl:disjointWith :Lieu-Dit ,
                              :Region .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Depression
:Depression rdf:type owl:Class ;
            rdfs:subClassOf :Phenomene_Meteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#DescriptionMeteo
:DescriptionMeteo rdf:type owl:Class ;
                  rdfs:comment "Classe pour décrire un état métérologique" .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#East
:East rdf:type owl:Class ;
      rdfs:subClassOf :WindDirectionValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Ensoleillé
:Ensoleillé rdf:type owl:Class ;
            rdfs:subClassOf :EtatDuCiel .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#EtatDuCiel
:EtatDuCiel rdf:type owl:Class ;
            rdfs:subClassOf :DescriptionMeteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#FaibleEclaircie
:FaibleEclaircie rdf:type owl:Class ;
                 rdfs:subClassOf :EtatDuCiel .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Grèle
:Grèle rdf:type owl:Class ;
       rdfs:subClassOf :Précipitations .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#HeavyRain
:HeavyRain rdf:type owl:Class ;
           rdfs:subClassOf :RainValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#HighSnowLevel
:HighSnowLevel rdf:type owl:Class ;
               rdfs:subClassOf :SnowLevelValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#HighTemperature
:HighTemperature rdf:type owl:Class ;
                 rdfs:subClassOf :TemperatureValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#HighVisibility
:HighVisibility rdf:type owl:Class ;
                rdfs:subClassOf :VisibilityValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#HighWindSpeed
:HighWindSpeed rdf:type owl:Class ;
               rdfs:subClassOf :WindSpeedValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Jour
:Jour rdf:type owl:Class ;
      rdfs:subClassOf :Lumière .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Lieu
:Lieu rdf:type owl:Class ;
      owl:disjointWith :MesureMeteo ,
                       :MesurePhysique ,
                       :Unit ;
      rdfs:label "Lieu"@fr ,
                 "Place"@en .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Lieu-Dit
:Lieu-Dit rdf:type owl:Class ;
          rdfs:subClassOf :Lieu ;
          owl:disjointWith :Region ;
          owl:hasKey ( :latitude
                       :longitude
                     ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#LowRain
:LowRain rdf:type owl:Class ;
         rdfs:subClassOf :RainValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#LowSnowLevel
:LowSnowLevel rdf:type owl:Class ;
              rdfs:subClassOf :SnowLevelValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#LowTemperature
:LowTemperature rdf:type owl:Class ;
                rdfs:subClassOf :TemperatureValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#LowVisibility
:LowVisibility rdf:type owl:Class ;
               rdfs:subClassOf :VisibilityValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Lumière
:Lumière rdf:type owl:Class ;
         rdfs:subClassOf :DescriptionMeteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureAngle
:MesureAngle rdf:type owl:Class ;
             rdfs:subClassOf :MesurePhysique .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureDirection
:MesureDirection rdf:type owl:Class ;
                 rdfs:subClassOf :MesurePhysique .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureHumidité
:MesureHumidité rdf:type owl:Class ;
                rdfs:subClassOf :MesurePhysique ;
                owl:hasKey ( :aPourUnitéHumidité
                           ) ,
                           ( :aPourValeur
                           ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureMeteo
:MesureMeteo rdf:type owl:Class ;
             owl:disjointWith :MesurePhysique ,
                              :Unit ;
             owl:hasKey ( :aPourStationMétéo
                        ) ,
                        ( :aPourDate
                        ) ;
             rdfs:comment "Classe pour réprésenter des mesures métérologiques réalisées à des instants déterminés" .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesurePhysique
:MesurePhysique rdf:type owl:Class ;
                owl:disjointWith :Unit ;
                owl:hasKey ( :aPourUnité
                           ) ,
                           ( :aPourValeur
                           ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesurePression
:MesurePression rdf:type owl:Class ;
                rdfs:subClassOf :MesurePhysique ;
                owl:hasKey ( :aPourUnitéPression
                           ) ,
                           ( :aPourValeur
                           ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureTemperature
:MesureTemperature rdf:type owl:Class ;
                   rdfs:subClassOf :MesurePhysique ;
                   owl:hasKey ( :aPourUnitéTempérature
                              ) ,
                              ( :aPourValeur
                              ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#MesureVitesse
:MesureVitesse rdf:type owl:Class ;
               rdfs:subClassOf :MesurePhysique ;
               owl:hasKey ( :aPourValeur
                          ) .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Neige
:Neige rdf:type owl:Class ;
       rdfs:subClassOf :Précipitations .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#NormalRain
:NormalRain rdf:type owl:Class ;
            rdfs:subClassOf :RainValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#NormalSunshine
:NormalSunshine rdf:type owl:Class ;
                rdfs:subClassOf :SunShineValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#NormalTemperature
:NormalTemperature rdf:type owl:Class ;
                   rdfs:subClassOf :TemperatureValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#NormalVisibility
:NormalVisibility rdf:type owl:Class ;
                  rdfs:subClassOf :VisibilityValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#NormalWindSpeed
:NormalWindSpeed rdf:type owl:Class ;
                 rdfs:subClassOf :WindSpeedValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#North
:North rdf:type owl:Class ;
       rdfs:subClassOf :WindDirectionValuePartition .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Nuageux
:Nuageux rdf:type owl:Class ;
         rdfs:subClassOf :EtatDuCiel .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Nuit
:Nuit rdf:type owl:Class ;
      rdfs:subClassOf :Lumière .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Nébulosité
:Nébulosité rdf:type owl:Class ;
            rdfs:subClassOf :ObservationMeteo .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#ObservationMeteo
:ObservationMeteo rdf:type owl:Class ;
                  rdfs:comment "Classe qui correspond à ce qui peut être mesuré à un instant précis vis à vis de la météo: le vent, la luminosité,la pluviométrie,..." .


###  http://www.semanticweb.org/pierr/ontologies/2022/3/meteo-ontologies#Orage
:Orage rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :aPourPluviometrie ;
                                                    owl:allValuesFrom :VeryHeavyRain
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :aPourVitesseVent ;
                                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                                        owl:unionOf ( :HighWindSpeed
                                                                                      :VeryHighWindSpeed
                                                                                    )
                                                                      ]
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
